{
    "editor.fontFamily": "Iosevka Fixed, JetBrains Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.codeLensFontSize": 13,
    "editor.fontWeight": "normal",
    "terminal.integrated.fontFamily": "IosevkaTerm Nerd Font Mono",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontWeight": "normal",
    "editor.scrollbar.horizontalScrollbarSize": 8,
    "editor.scrollbar.verticalScrollbarSize": 8,
    "editor.minimap.enabled": false,
    "editor.tokenColorCustomizations": {
        "[Dracula Theme]": {
            "keywords": "#fabffa",
            "numbers": "#f8477c",
            "types": "#5bcfca",
            "strings": "#ccccff",
            "functions": "#ba6cfa",
            "variables": "#ca68bd",
            "textMateRules": [
                {
                    "scope": [
                        "punctuation.definition.string.begin",
                        "punctuation.definition.string.end"
                    ],
                    "settings": {
                        "foreground": "#ccccff"
                    }
                },
                {
                    "scope": [
                        "variable.other.readwrite.alias.ts",
                        "string.quoted.double.ts",
                        "variable.other.object.ts",
                        "string.quoted.single.ts"
                    ],
                    "settings": {
                        "foreground": "#f8477c",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "variable.other.readwrite.alias.tsx",
                        "string.quoted.double.ts",
                        "variable.other.object.ts",
                        "string.quoted.single.ts",
                    ],
                    "settings": {
                        "foreground": "#f8477c",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "meta.jsx.children.tsx"
                    ],
                    "settings": {
                        "foreground": "#48d6cf",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "punctuation.definition.tag.begin.tsx",
                        "punctuation.definition.tag.end.tsx",
                    ],
                    "settings": {
                        "foreground": "#FF79C6",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "entity.other.attribute-name.tsx",
                    ],
                    "settings": {
                        "foreground": "#ff96b1",
                        "fontStyle": "italic",
                    }
                }
            ]
        }
    },
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.guides.bracketPairs": "active",
    "editor.defaultColorDecorators": "auto",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.guides.bracketPairsHorizontal": true,
    "editor.colorDecoratorsActivatedOn": "hover",
    "editor.cursorBlinking": "expand",
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 2,
    "editor.smartSelect.selectSubwords": true,
    "editor.autoClosingBrackets": "always",
    "explorer.confirmDelete": false,
    "workbench.colorCustomizations": {
        "[Dracula Theme]": {
            "editorBracketPairGuide.activeBackground1": "#c681ff",
            "editorBracketPairGuide.activeBackground4": "#48d6cf",
            "editorBracketHighlight.foreground4": "#48d6cf",
            "editorWarning.foreground": "#fa68ff",
            "editorError.foreground": "#ff3e88",
            "editorBracketHighlight.foreground1": "#ba90ff",
        }
    },
    "explorer.confirmDragAndDrop": false,
    "typescript.updateImporjsonFileMove.enabled": "always",
    "editor.inlineSuggest.showToolbar": "always",
    "workbench.iconTheme": "vscode-icons",
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "workbench.colorTheme": "Dracula Theme",
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
    "debug.console.fontSize": 13,
    "debug.console.fontFamily": "Iosevka",
    "debug.enableStatusBarColor": true,
    "[markdown]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
}